{
	"name": "bookmarks98",
	"version": "1.0.0",
	"description": "",
	"main": ".eleventy.js",
	"scripts": {
		"clean": "rm -rf dist",
		"eleventy:build": "npx @11ty/eleventy",
		"eleventy:watch": "npx @11ty/eleventy --serve",
		"eleventy:debug": "DEBUG=* eleventy",
		"sass:build": "sass --no-source-map --style=compressed src/assets/scss:src/assets/css",
		"sass:watch": "sass --watch --no-source-map src/assets/scss:src/assets/css",
		"build": "npm-run-all clean sass:build eleventy:build",
		"start": "npm-run-all clean --parallel sass:watch eleventy:watch"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@11ty/eleventy": "^2.0.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"prettier": "3.2.4",
		"sass": "^1.75.0"
	},
	"dependencies": {
		"npm-run-all": "^4.1.5"
	}
}
